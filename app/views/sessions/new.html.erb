<h1>Login</h1>

<div class="jumbotron text-center">
    <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email %> <br>
    <%= f.text_field :email %> <br>

    <%= f.label :password %> <br>
    <%= f.text_field :password %> <br>

    <%= f.label :password_confirmation %> <br>
    <%= f.text_field :password_confirmation %> <br>

    <%= f.submit "Login"%>
    <% end %>
</div>
