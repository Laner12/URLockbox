<h1>Create a new user account</h1>

<div class="jumbotron text-center">
    <%= form_for(@user) do |f| %>
    <%= f.label :email %> <br>
    <%= f.text_field :email %> <br>

    <%= f.label :password %> <br>
    <%= f.text_field :password %> <br>

    <%= f.label :password_confirmation %> <br>
    <%= f.text_field :password_confirmation %> <br>

    <%= f.submit "Create Account"%>
    <% end %>
</div>
